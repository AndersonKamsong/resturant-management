            content: '';
            position: absolute;
            top: 0; left: 0;
            width: 100%; height: 100%;
            background-color: rgba(0, 0, 0, 0.6);
            z-index: 1;
        }
        .hero-content {
            position: relative;
            z-index: 2;
        }
        .card {
            background-color: #2a2a2a;
            color: #fff;
            border: 1px solid #ffd700;
        }
        .text-gold {
            color: #ffd700;
        }
        footer {
            background-color: #111;
            color: #ffd700;
            text-align: center;
            padding: 1rem 0;
        }
