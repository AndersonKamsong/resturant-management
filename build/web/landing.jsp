    <style>
        body {
            background-color: #1c1c1c;
            color: #fff;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        .navbar {
            background-color: #111;
        }
        .navbar-brand, .nav-link, .btn-outline-gold {
            color: #ffd700 !important;
        }
        .btn-outline-gold {
