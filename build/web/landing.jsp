<!-- Hero Section -->
<section class="hero">
    <div class="hero-content" data-aos="fade-up">
        <h1 class="display-3">Découvrez la Saveur</h1>
        <p class="lead">Plats traditionnels camerounais préparés avec passion</p>
        <a href="#menu" class="btn btn-gold mt-3">Voir le Menu</a>
    </div>
</section>

<!-- Menu Section -->
<section id="menu" class="py-5">
    <div class="container" data-aos="fade-up">
        <h2 class="text-center mb-4">Nos Spécialités</h2>
        <div class="row g-4">
            <div class="col-md-4" data-aos="zoom-in">
                <div class="card">
                    <img src="data:image/jpeg;base64,"
                         class="card-img-top" alt="Ndolè">
                    <div class="card-body">
                        <h5 class="card-title text-gold">Ndolè</h5>
                        <p class="card-text">Le plat national camerounais à base de feuilles de ndolè, arachides, et viande ou poisson.</p>
                        <p class="text-gold">5,000 FCFA</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4" data-aos="zoom-in">
                <div class="card">
                    <img src="data:image/jpeg;base64," 
                         class="card-img-top" alt="Poulet DG">
                    <div class="card-body">
                        <h5 class="card-title text-gold">Poulet DG</h5>
                        <p class="card-text">Poulet mijoté avec des plantains, légumes et épices, un plat de fête.</p>
                        <p class="text-gold">6,500 FCFA</p>
                    </div>
                </div>
            </div>
            <div class="col-md-4" data-aos="zoom-in">
                <div class="card">
                    <img src="data:image/jpeg;base64,"
                         class="card-img-top" alt="Koki">
                    <div class="card-body">
                        <h5 class="card-title text-gold">Koki</h5>
                        <p class="card-text">Gâteau de haricots noirs cuit à la vapeur dans des feuilles de bananier.</p>
                        <p class="text-gold">3,500 FCFA</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Available Tables -->
<section id="tables" class="py-5">
    <div class="container" data-aos="zoom-in">
        <h2 class="text-center mb-4">Réserver une Table</h2>
        <form>
            <div class="row g-3">
                <div class="col-md-6">
                    <input type="text" class="form-control" placeholder="Votre Nom" required>
                </div>
                <div class="col-md-6">
                    <input type="email" class="form-control" placeholder="Email" required>
                </div>
                <div class="col-md-6">
                    <input type="date" class="form-control" required>
                </div>
                <div class="col-md-6">
                    <input type="time" class="form-control" required>
                </div>
                <div class="col-md-12">
                    <select class="form-control" required>
                        <option selected disabled>Nombre de personnes</option>
                        <option>2 Personnes</option>
                        <option>4 Personnes</option>
                        <option>6 Personnes</option>
                        <option>8+ Personnes</option>
                    </select>
                </div>
                <div class="col-12 text-center">
                    <button type="submit" class="btn btn-gold">Réserver</button>
                </div>
            </div>
        </form>
    </div>
</section>

<!-- Promotional Menu -->
<section id="promotions" class="py-5 bg-black">
    <div class="container" data-aos="fade-up">
        <h2 class="text-center mb-4 text-gold">Menu Promotionnel</h2>
        <div class="row g-4">
            <div class="col-md-4" data-aos="fade-up">
                <div class="card">
                    <img src="data:image/jpeg;base64,"
                         class="card-img-top" alt="Brochettes">
                    <div class="card-body">
                        <h5 class="card-title text-gold">Brochettes + Boisson</h5>
                        <p class="card-text">Brochettes de boeuf ou poulet accompagnées d'une boisson locale.</p>
                        <p class="text-gold">3,500 FCFA <small class="text-muted"><del>5,000 FCFA</del></small></p>
                    </div>
                </div>
            </div>
            <div class="col-md-4" data-aos="fade-up" data-aos-delay="100">
                <div class="card">
                    <img src="data:image/jpeg;base64,"
                         class="card-img-top" alt="Eru">
                    <div class="card-body">
                        <h5 class="card-title text-gold">Eru Spécial</h5>
                        <p class="card-text">Plat traditionnel de l'Ouest avec viande fumée et poisson.</p>
                        <p class="text-gold">4,000 FCFA <small class="text-muted"><del>5,500 FCFA</del></small></p>
                    </div>
                </div>
            </div>
            <div class="col-md-4" data-aos="fade-up" data-aos-delay="200">
                <div class="card">
                    <img src="data:image/jpeg;base64,"
                         class="card-img-top" alt="Beignets">
                    <div class="card-body">
                        <h5 class="card-title text-gold">Beignets + Haricots</h5>
                        <p class="card-text">Beignets de haricots ou banane plantain avec thé local.</p>
                        <p class="text-gold">2,000 FCFA <small class="text-muted"><del>3,000 FCFA</del></small></p>
                    </div>
                </div>
            </div>
        </div>
